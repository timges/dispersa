  "spacing": {
    "scale": {
      "$description": "Spacing scale from Spectrum layout.json",
      "50": { "$type": "dimension", "$value": { "value": 2, "unit": "px" } },
      "100": { "$type": "dimension", "$value": { "value": 8, "unit": "px" } },
      "200": { "$type": "dimension", "$value": { "value": 12, "unit": "px" } },
      "300": { "$type": "dimension", "$value": { "value": 16, "unit": "px" } },
      "400": { "$type": "dimension", "$value": { "value": 24, "unit": "px" } },
      "500": { "$type": "dimension", "$value": { "value": 32, "unit": "px" } },
      "600": { "$type": "dimension", "$value": { "value": 40, "unit": "px" } }
    }
  },
  "typography": {
    "$description": "Typography tokens from Spectrum typography.json",
    "font": {
      "family": {
        "sans": {
          "$type": "fontFamily",
          "$value": ["Adobe Clean", "ui-sans-serif", "system-ui", "sans-serif"],
          "$description": "Spectrum sans-serif font stack"
        },
        "serif": {
          "$type": "fontFamily",
          "$value": ["Adobe Clean Serif", "ui-serif", "Georgia", "serif"],
          "$description": "Spectrum serif font stack"
        }
      },
      "weight": {
        "light": { "$type": "fontWeight", "$value": 300 },
        "regular": { "$type": "fontWeight", "$value": 400 },
        "medium": { "$type": "fontWeight", "$value": 500 },
        "bold": { "$type": "fontWeight", "$value": 700 },
        "extra-bold": { "$type": "fontWeight", "$value": 800 },
        "black": { "$type": "fontWeight", "$value": 900 }
      },
      "size": {
        "$description": "Desktop font size scale from Spectrum",
        "75": { "$type": "dimension", "$value": { "value": 12, "unit": "px" } },
        "100": { "$type": "dimension", "$value": { "value": 14, "unit": "px" } },
        "200": { "$type": "dimension", "$value": { "value": 16, "unit": "px" } },
        "300": { "$type": "dimension", "$value": { "value": 18, "unit": "px" } },
        "400": { "$type": "dimension", "$value": { "value": 20, "unit": "px" } },
        "500": { "$type": "dimension", "$value": { "value": 22, "unit": "px" } }
      }
    }
  },
  "layout": {
    "$description": "Layout tokens from Spectrum layout.json",
    "corner-radius": {
      "75": { "$type": "dimension", "$value": { "value": 3, "unit": "px" } },
      "100": { "$type": "dimension", "$value": { "value": 4, "unit": "px" } },
      "200": { "$type": "dimension", "$value": { "value": 5, "unit": "px" } },
      "500": { "$type": "dimension", "$value": { "value": 8, "unit": "px" } },
      "800": { "$type": "dimension", "$value": { "value": 16, "unit": "px" } }
    },
    "border-width": {
      "100": { "$type": "dimension", "$value": { "value": 1, "unit": "px" } },
      "200": { "$type": "dimension", "$value": { "value": 2, "unit": "px" } }
    }
  },
  "shadow": {
    "$description": "Elevation shadows derived from Spectrum drop-shadow tokens",
    "raw": {
      "100": {
        "$type": "shadow",
        "$value": {
          "offsetX": { "value": 0, "unit": "px" },
          "offsetY": { "value": 1, "unit": "px" },
          "blur": { "value": 4, "unit": "px" },
          "spread": { "value": 0, "unit": "px" },
          "color": { "colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.12 }
        },
        "$description": "Subtle card shadow"
      },
      "200": {
        "$type": "shadow",
        "$value": {
          "offsetX": { "value": 0, "unit": "px" },
          "offsetY": { "value": 4, "unit": "px" },
          "blur": { "value": 16, "unit": "px" },
          "spread": { "value": 0, "unit": "px" },
          "color": { "colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.16 }
        },
        "$description": "Popover / dropdown shadow"
      },
      "300": {
        "$type": "shadow",
        "$value": {
          "offsetX": { "value": 0, "unit": "px" },
          "offsetY": { "value": 8, "unit": "px" },
          "blur": { "value": 28, "unit": "px" },
          "spread": { "value": 0, "unit": "px" },
          "color": { "colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.24 }
        },
        "$description": "Modal shadow"
      }
    }
  },
  "motion": {
    "$description": "Animation duration tokens",
    "duration": {
      "100": { "$type": "duration", "$value": { "value": 130, "unit": "ms" } },
      "200": { "$type": "duration", "$value": { "value": 250, "unit": "ms" } },
      "300": { "$type": "duration", "$value": { "value": 400, "unit": "ms" } }
    }
  }
}
