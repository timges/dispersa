{
  "$description": "Comfortable density overrides (more spacious spacing + paddings)",
  "$extensions": { "com.example.density": "comfortable" },
  "space": {
    "50": { "$type": "dimension", "$value": { "value": 6, "unit": "px" } },
    "100": { "$type": "dimension", "$value": { "value": 10, "unit": "px" } },
    "150": { "$type": "dimension", "$value": { "value": 14, "unit": "px" } },
    "200": { "$type": "dimension", "$value": { "value": 18, "unit": "px" } },
    "300": { "$type": "dimension", "$value": { "value": 28, "unit": "px" } },
    "400": { "$type": "dimension", "$value": { "value": 36, "unit": "px" } }
  },
  "surface": {
    "card": {
      "padding": { "$type": "dimension", "$value": "{space.300}" }
    }
  }
}
